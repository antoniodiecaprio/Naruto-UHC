        </dependency>
        <dependency>
            <groupId>fr.lyneris</groupId>
            <artifactId>Lyneris-UHC</artifactId>
            <version>0.1</version>
            <systemPath>${project.basedir}/../Lyneris-UHC/target/UHC-API-0.1.jar</systemPath>
            <scope>system</scope>
        </dependency>
        <dependency>
            <groupId>fr.lyneris</groupId>
            <artifactId>Common</artifactId>
            <version>2.0</version>
            <systemPath>${project.basedir}/../Lyneris-Common/target/Common-2.0.jar</systemPath>
